{
    "name": "my-projects",
    "version": "0.1.0",
    "private": true,
    "description": "My Projects",
    "type": "module",
    "workspaces": [
        "apps/*",
        "packages/cms/*",
        "packages/config/*",
        "packages/lib/*",
        "packages/ui/*",
        "packages/lab/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "deps:check": "knip",
        "dev": "turbo run dev",
        "format": "prettier . --write --cache --ignore-unknown",
        "format:check": "prettier . --check --cache --ignore-unknown",
        "gen-commit": "node scripts/gen-commit/index.mjs --copy",
        "postinstall": "lefthook install",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint -- --fix",
        "publish": "dotenv -e .env changeset publish",
        "typecheck": "turbo run typecheck",
        "version": "changeset && changeset version && npm i"
    },
    "devDependencies": {
        "@changesets/cli": "2.29.5",
        "@commitlint/cli": "^18.0.0",
        "@commitlint/config-conventional": "^18.0.0",
        "@mansur-gabidullin/eslint-config": "*",
        "@mansur-gabidullin/prettier-config": "*",
        "@mansur-gabidullin/typescript-config": "*",
        "@mansur-gabidullin/vitest-config": "*",
        "clipboardy": "^4.0.0",
        "commander": "^14.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv-cli": "^8.0.0",
        "inquirer": "^12.6.3",
        "knip": "5.61.2",
        "lefthook": "^1.11.14",
        "turbo": "^2.5.4"
    },
    "packageManager": "npm@10.9.2",
    "engines": {
        "node": ">=18"
    }
}
